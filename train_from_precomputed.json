{
    "model": {
        "pretrained_model": "MIT/ast-finetuned-audioset-10-10-0.4593",
        "hidden_sizes": [
            512,
            128
        ],
        "activation": "relu",
        "dropout_rate": 0.15,
        "output_dim": 128
    },
    "training": {
        "batch_size": 2,
        "epochs": 20,
        "learning_rate": 2e-5,
        "weight_decay": 0.05,
        "gradient_accumulation_steps": 12,
        "warmup_ratio": 0.2,
        "triplet_margin": 0.5,
        "num_workers": 6,
        "pin_memory": true,
        "eval_strategy": "steps",
        "save_strategy": "steps",
        "logging_steps": 50,
        "save_steps": 500,
        "eval_steps": 200,
        "seed": 42,
        "force_single_gpu": true
    },
    "data": {
        "chunks_dir": "PROJECT/precomputed_AST",
        "test_split_ratio": 0.1,
        "min_files_per_genre": 2,
        "max_positive_tracks": 12,
        "triplets_per_positive_track": 3,
        "enable_feature_caching": false,
        "cache_train_dataset": false,
        "cache_test_dataset": false,
        "preload_chunks": false
    },
    "paths": {
        "model_output_dir": "ast_triplet_output",
        "log_dir": "logs",
        "checkpoint_dir": null
    },
    "experiment_name": "from_precomputed_pt",
    "description": "Training from precomputed AST inputs"
}