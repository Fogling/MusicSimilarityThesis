{
    "model": {
        "pretrained_model": "MIT/ast-finetuned-audioset-10-10-0.4593",
        "projection_hidden_layers": [
            512
        ],
        "projection_activation": "relu",
        "projection_dropout_rate": 0.15,
        "output_dim": 128
    },
    "training": {
        "batch_size": 24,
        "epochs": 10,
        "learning_rate": 2e-05,
        "weight_decay": 0.05,
        "gradient_accumulation_steps": 3,
        "triplet_margin": 0.5,
        "margin_schedule_end_epoch": 1,
        "margin_schedule_max": 0.5,
        "gradient_clip_norm": 10,
        "use_custom_lr": true,
        "head_lr_multiplier": 1.0,
        "multiplier_converge_epoch": 1,
        "warmup_steps_pct": 0.1,
        "min_lr": 1e-07,
        "use_cosine_scheduler": false,
        "num_workers": 12,
        "pin_memory": true,
        "prefetch_factor": 8,
        "persistent_workers": true,
        "logging_strategy": "epoch",
        "logging_steps": 100,
        "eval_strategy": "epoch",
        "save_strategy": "no",
        "save_steps": 1000,
        "eval_steps": 500,
        "seed": 13,
        "disable_tqdm": true,
        "force_single_gpu": true,
        "bf16": true,
        "fp16": false,
        "tf32": true,
        "allow_tf32_matmul": true,
        "allow_tf32_cudnn": true,
        "set_float32_matmul_precision": "high",
        "enable_early_stopping": true,
        "early_stopping_patience": 3,
        "early_stopping_min_delta": 0.01,
        "post_resample_grace_epochs": 2
    },
    "data": {
        "chunks_dir": "precomputed_7G_2Chunk",
        "split_file_train": null,
        "split_file_test": null,
        "test_split_ratio": 0.2,
        "min_files_per_genre": 2,
        "max_positive_tracks": 2,
        "triplets_per_positive_track": 5,
        "resample_train_samples": false,
        "resample_fraction": 0.15,
        "resample_cadence": 2,
        "resample_start_epoch": 1,
        "resample_schedule_override": null,
        "negative_mining": "none",
        "negative_mining_start_epoch": 2,
        "stratified_batching": true,
        "min_per_subgenre": 2,
        "quick_test_run": false,
        "quick_test_fraction": 0.05,
        "use_legacy_splits": false,
        "legacy_split_dir": null
    },
    "paths": {
        "model_output_dir": "ast_triplet_output",
        "log_dir": "logs",
        "checkpoint_dir": null
    },
    "experiment_name": "Testing 2 chunks per track",
    "description": "Dataset size 2x5"
}