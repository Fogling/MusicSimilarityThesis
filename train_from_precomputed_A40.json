{
    "model": {
        "pretrained_model": "MIT/ast-finetuned-audioset-10-10-0.4593",
        "hidden_sizes": [
            512,
            128
        ],
        "activation": "relu",
        "dropout_rate": 0.15,
        "output_dim": 128
    },
    "training": {
        "batch_size": 24,
        "epochs": 15,
        "learning_rate": 2e-5,
        "weight_decay": 0.05,
        "gradient_accumulation_steps": 2,
        "warmup_ratio": 0.1,
        "triplet_margin": 0.5,
        "num_workers": 12,
        "pin_memory": true,
        "prefetch_factor": 8,
        "persistent_workers": true,
        "eval_strategy": "epoch",
        "save_strategy": "no",
        "logging_steps": 500,
        "save_steps": 1000,
        "eval_steps": 500,
        "disable_tqdm": false,
        "seed": 42,
        "force_single_gpu": true,
        "bf16": true,
        "fp16": false,
        "tf32": true,
        "allow_tf32_matmul": true,
        "allow_tf32_cudnn": true,
        "set_float32_matmul_precision": "high"
    },
    "data": {
        "chunks_dir": "precomputed_AST",
        "test_split_ratio": 0.15,
        "min_files_per_genre": 2,
        "max_positive_tracks": 6,
        "triplets_per_positive_track": 1,
        "resample_each_epoch": true,
        "negative_mining": "semi_hard"
    },
    "paths": {
        "model_output_dir": "ast_triplet_output",
        "log_dir": "logs",
        "checkpoint_dir": null
    },
    "experiment_name": "from_precomputed_A40_bf16",
    "description": "A40: bf16+TF32, larger batch, higher dataloader throughput"
}