{
    "model": {
        "pretrained_model": "MIT/ast-finetuned-audioset-10-10-0.4593",
        "hidden_sizes": [
            512,
            128
        ],
        "activation": "relu",
        "dropout_rate": 0.3,
        "output_dim": 128
    },
    "training": {
        "batch_size": 24,
        "epochs": 15,
        "learning_rate": 1e-5,
        "weight_decay": 0.1,
        "gradient_accumulation_steps": 3,
        "triplet_margin": 0.3,
        "margin_schedule_end_epoch": 6,
        "margin_schedule_max": 0.7,
        "gradient_clip_norm": 1.0,
        "head_lr_multiplier": 4.0,
        "multiplier_converge_epoch": 3,
        "warmup_steps_pct": 0.05,
        "min_lr": 1e-6,
        "use_cosine_scheduler": true,
        "num_workers": 12,
        "pin_memory": true,
        "prefetch_factor": 8,
        "persistent_workers": true,
        "eval_strategy": "epoch",
        "save_strategy": "no",
        "logging_steps": 100,
        "save_steps": 1000,
        "eval_steps": 500,
        "disable_tqdm": true,
        "seed": 42,
        "force_single_gpu": true,
        "bf16": true,
        "fp16": false,
        "tf32": true,
        "allow_tf32_matmul": true,
        "allow_tf32_cudnn": true,
        "set_float32_matmul_precision": "high"
    },
    "data": {
        "chunks_dir": "precomputed_AST",
        "test_split_ratio": 0.15,
        "min_files_per_genre": 2,
        "max_positive_tracks": 6,
        "triplets_per_positive_track": 1,
        "resample_each_epoch": false,
        "negative_mining": "none",
        "stratified_batching": false,
        "min_per_subgenre": 2
    },
    "paths": {
        "model_output_dir": "ast_triplet_output",
        "log_dir": "logs",
        "checkpoint_dir": null
    },
    "experiment_name": "testing different values for dropout, margin, LR",
    "description": "stratified batching on"
}