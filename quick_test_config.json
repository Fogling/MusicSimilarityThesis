{
  "model": {
    "pretrained_model": "MIT/ast-finetuned-audioset-10-10-0.4593",
    "hidden_sizes": [
      512,
      128
    ],
    "activation": "relu",
    "dropout_rate": 0.15,
    "output_dim": 128
  },
  "training": {
    "batch_size": 2,
    "epochs": 3,
    "learning_rate": 2e-05,
    "weight_decay": 0.05,
    "gradient_accumulation_steps": 1,
    "warmup_ratio": 0.1,
    "triplet_margin": 0.5,
    "num_workers": 8,
    "pin_memory": true,
    "prefetch_factor": 2,
    "persistent_workers": true,
    "eval_strategy": "steps",
    "save_strategy": "no",
    "logging_steps": 10,
    "seed": 42,
    "force_single_gpu": true
  },
  "data": {
    "chunks_dir": "WAV",
    "split_file_train": null,
    "split_file_test": null,
    "test_split_ratio": 0.1,
    "min_files_per_genre": 2,
    "max_positive_tracks": 3,
    "triplets_per_positive_track": 1,
    "quick_test_run": true,
    "quick_test_fraction": 0.07,
    "use_legacy_splits": false,
    "legacy_split_dir": null
  },
  "paths": {
    "model_output_dir": "test_output",
    "log_dir": "test_logs",
    "checkpoint_dir": null
  },
  "experiment_name": "quick_test_debug",
  "description": "Quick test run with 5% data, debug logging, minimal settings for error detection, single GPU mode"
}